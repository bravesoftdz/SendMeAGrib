        </Reference>
        <Reference Include="libNougat.fx"/>
        <Reference Include="libSugar.fx"/>
        <Reference Include="libxml2.fx"/>
    </ItemGroup>
    <ItemGroup>
        <Compile Include="AppDelegate.pas"/>
        <Compile Include="FlipsideViewController.pas"/>
        <Compile Include="MainViewController.pas"/>
